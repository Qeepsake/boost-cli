{
  "name": "@react-native-boost/boost-cli",
  "description": "CLI tool to build the wireframes of components and utilities for a React/ React Native project",
  "version": "0.0.1",
  "author": "Denise @Denise-Ng",
  "bin": {
    "boost-cli": "bin/run"
  },
  "bugs": {
    "url": "https://github.com/React-Native-Boost/boost-cli/issues"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^10.17.51",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/React-Native-Boost/boost-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "boost-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/React-Native-Boost/boost-cli.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "mkdirp": "^0.5.5",
    "tslib": "^1.14.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
